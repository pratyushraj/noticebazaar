{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "src/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/index.ts"
    },
    {
      "source": "/health",
      "destination": "/src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

